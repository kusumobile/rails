<h1>Log In</h1>

<%= form_for(:session, url: login_path) do |f| %>
  <p>
    <%= f.label :email %><br />
    <%= f.email_field :email %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
  </p>
  <p>
    <%= f.submit "Log In" %>
  </p>
<% end %>

<%= link_to "Back to article listings", articles_path %>
